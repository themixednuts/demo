{
  "name": "@mixednuts/demo",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./buffers": {
      "import": "./dist/buffers/index.js",
      "types": "./dist/buffers/index.d.ts"
    },
    "./gen/*": {
      "import": "./dist/gen/*.js",
      "types": "./dist/gen/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.55.1",
    "@bufbuild/protoc-gen-es": "^2.6.0",
    "@deno/vite-plugin": "^1.0.5",
    "@types/deno": "^2.3.0",
    "typescript": "~5.8.3",
    "unplugin-dts": "^1.0.0-beta.0",
    "vite": "^7.0.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.6.0"
  }
}
